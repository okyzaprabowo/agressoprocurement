@model IEnumerable<LenProcurementApp.Models.EditUserViewModel>
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@Html.Partial("_SumStruktural")
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
            <h3 class="box-title">Table Data Terbaru</h3>
            <!-- Date and time range -->
            <div class="form-group pull-right" style="margin-bottom:0;">
                <div class="input-group">
                    <button class="btn btn-info pull-right" id="daterange-btn">
                        <i class="fa fa-calendar"></i> Filter Tanggal 
                        <i class="fa fa-caret-down"></i>
                    </button>
                </div>
            </div><!-- /.form group -->
            </div>
            <div class="box-body">
                <table id="" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>No</th>
                            <th>User</th>
                            <th>Jml. Item</th>
                            <th>PLK</th>
                            <th>Divisi</th>
                            <th>JobCode</th>
                            <th>JobCode(T)</th>
                            <th>DPB</th>
                            <th>SPPH</th>
                            <th>PO</th>
                            <th>Supplier(T)</th>
                            <th>BAPB</th>
                            <th>B. Tiba</th>
                            <th>SPP</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{ 
                            int i = 0;
                        }
                        @foreach (var item in Model)
                        {
                            i++;
                            <tr>
                                <td>@i</td>
                                <td>User @i</td>
                                <td>Jml. Item @i</td>
                                <td>PLK @i</td>
                                <td>Divisi @i</td>
                                <td>JobCode @i</td>
                                <td>JobCode T @i</td>
                                <td><a href="#"> DPB @i </a></td>
                                <td><a href="#"> SPPH @i </a></td>
                                <td><a href="#"> PO @i </a></td>
                                <td><a href="#" class="len-danger"> Supplier @i </a></td>
                                <td><a href="#"> BAPB @i </a></td>
                                <td><a href="#"> B. Tiba @i </a></td>
                                <td><a href="#"> SPP @i </a></td>
                                @*<td>
                                    @Html.DisplayFor(modelItem => item.user_id)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.full_name)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.UserName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.initials)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.position)
                                </td>*@
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

