
@{
    ViewBag.Title = "Persiapan Print";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="print-modal">
            <div id="print-modal" class="modal fade" role="dialog">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            @*<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>*@
                            <h4 class="modal-title">Persiapan Print</h4>
                        </div>
                        <form class="form-horizontal">
                            <div class="modal-body">
                                <div class="form-group">
                                    <label for="inputEmail3" class="col-sm-2 control-label">Pilih PO</label>
                                    <div class="col-sm-10">
                                        <select class="form-control select2" multiple="multiple" data-placeholder="Select a State" style="width: 100%;">
                                            <option>Alabama</option>
                                            <option>Alaska</option>
                                            <option>California</option>
                                            <option>Delaware</option>
                                            <option>Tennessee</option>
                                            <option>Texas</option>
                                            <option>Washington</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Kembali</button>
                                <button type="button" class="btn btn-primary">Print</button>
                            </div>
                        </form>
                    </div><!-- /.modal-content -->
                </div><!-- /.modal-dialog -->
            </div>
        </div>
    </div>
</div>
@section Styles {
    <!-- Select2 -->
    <link rel="stylesheet" href="~/Scripts/Import/select2/select2.min.css">
}
@section Scripts {
    <!-- Select2 -->
    <script src="~/Scripts/Import/select2/select2.full.min.js"></script>
    <script type="text/javascript">
        //Initialize Select2 Elements
        $(".select2").select2();
        $("#print-modal").show();
    </script>
}